<p id="notice"><%= notice %></p>

<h1>
  <%= @user.name %>'s Parties
</h1>

<table>
  <thead>
    <tr>
      <th>Party</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.own_events.each do |t| %>
      <tr>
        <td><%= t.name %></td>
        <td><%= link_to 'Show', "/events/#{t.id}", method: :get %></td>
      </tr>
    <% end %>
  </tbody>
</table>



<% if @user.id == session[:user_id]%>
  <%= button_to 'New Event', '/events/new', method: :get %>
<% end %>
<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
